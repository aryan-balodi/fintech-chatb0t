{
  "category": "ONBOARDING BUSINESS",
  "service_name": "GST Basic Details",
  "description": "This service retrieves GST details, including legal name, trade name, PAN, and business activities, for compliance and onboarding workflows.",
  "llm_summary": "GST Basic Details provides comprehensive GST information, including legal and trade names, PAN, business activities, and jurisdiction details, aiding in business onboarding and compliance.",
  "tags": [
    "gst details",
    "business verification",
    "compliance",
    "onboarding api",
    "taxpayer information",
    "gstin lookup"
  ],
  "use_cases": [
    "Validate GSTIN details during business onboarding.",
    "Retrieve legal and trade names for compliance workflows.",
    "Verify taxpayer type and GSTIN status for regulatory purposes.",
    "Support due diligence with detailed GST information.",
    "Ensure accurate business verification for financial services."
  ],
  "request_schema": [
    {
      "field": "gstin",
      "type": "string",
      "description": "GSTIN (Goods and Services Tax Identification Number) to fetch details.",
      "validations": {
        "pattern": "^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing GST details.",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response.",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API.",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed.",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service.",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
      "field": "data",
      "type": "object",
      "description": "GST details.",
