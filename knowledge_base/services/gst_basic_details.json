        },
        "legalName": {
          "type": "string",
          "description": "Legal name of the business."
        },
        "tradeName": {
          "type": "string",
          "description": "Trade name of the business."
        },
        "panNumber": {
          "type": "string",
          "description": "PAN number associated with the GSTIN."
        },
        "constitutionOfBusiness": {
          "type": "string",
