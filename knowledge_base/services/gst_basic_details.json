    "gst details",
    "company verification",
    "mca lookup",
    "din lookup",
    "business onboarding",
    "compliance"
  ],
  "use_cases": [
    "Obtain official company details using a director's DIN as part of business onboarding.",
    "Validate company registration information using Ministry of Corporate Affairs data.",
    "Assist in due diligence or fraud prevention by fetching company identity records from DIN."
