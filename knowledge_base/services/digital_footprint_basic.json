{
  "category": "ALTERNATE DATA SUITE",
  "service_name": "Digital Footprint Basic (Phone-Email-Name)",
  "description": "This service provides insights into the digital footprint of an individual based on their phone number, email, and name. It retrieves the first-seen year for various combinations of these identifiers, enabling alternate data-based verification workflows.",
  "llm_summary": "Digital Footprint Basic service analyzes the digital presence of an individual using their phone number, email, and name. It provides first-seen year data for alternate data verification and compliance workflows.",
  "tags": [
    "digital footprint",
    "alternate data",
    "phone-email-name",
    "compliance",
    "onboarding api",
    "data verification",
    "secure kyc"
  ],
  "use_cases": [
    "Verify the digital presence of an individual during onboarding workflows.",
    "Retrieve first-seen year data for compliance and fraud prevention.",
    "Support alternate data-based KYC processes in financial services and insurance.",
    "Enable identity verification workflows in government and private sectors."
  ],
  "request_schema": [
    {
      "field": "mobileNumber",
      "type": "string",
      "description": "Mobile number to analyze digital footprint.",
      "validations": {
        "minLength": 10,
        "maxLength": 10,
        "pattern": "\\d{10}"
      },
      "required": true
    },
    {
      "field": "email",
      "type": "string",
      "description": "Email address to analyze digital footprint.",
      "validations": {
        "pattern": "^[\\w.%+-]+@[\\w.-]+\\.[a-zA-Z]{2,}$"
      },
      "required": true
    },
    {
      "field": "name",
      "type": "string",
      "description": "Name of the individual to analyze digital footprint.",
      "validations": {
        "minLength": 1
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing digital footprint data",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
      "field": "data",
      "type": "object",
      "description": "Digital footprint data",
      "properties": {
        "phoneFirstSeenYear": {
          "type": "string",
          "description": "Year when the phone number was first seen"
        },
        "phoneNameFirstSeenYear": {
          "type": "string",
          "description": "Year when the phone number and name combination was first seen"
        },
        "emailFirstSeenYear": {
          "type": "string",
          "description": "Year when the email address was first seen"
        },
        "emailNameFirstSeenYear": {
          "type": "string",
          "description": "Year when the email address and name combination was first seen"
        },
        "phoneEmailFirstSeenYear": {
          "type": "string",
          "description": "Year when the phone number and email combination was first seen"
        }
      },
      "required": true
    },
    {
      "field": "vendorResponse",
      "type": "array",
      "description": "Response details from vendors",
      "items": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "integer",
            "description": "Vendor-specific status code"
          },
          "name": {
            "type": "string",
            "description": "Vendor name"
          },
          "sequence": {
            "type": "integer",
            "description": "Vendor sequence number"
          }
        }
      },
      "required": true
    }
  ],
  "example_request": {
    "input": {
      "mobileNumber": "9870963498",
      "email": "pranilpalan@gmail.com",
      "name": "Pranil Palan",
      "consent": true
    }
  },
  "example_response": {
    "status": "success",
    "requestId": "8c205aa4-320a-475d-8d0d-5e36e6d81482",
    "type": "point",
    "serviceStatusCode": 200,
    "data": {
      "phoneFirstSeenYear": "2011",
      "phoneNameFirstSeenYear": "2011",
      "emailFirstSeenYear": "2010",
      "emailNameFirstSeenYear": "2010",
      "phoneEmailFirstSeenYear": "2014"
    },
    "vendorResponse": [
      {
        "statusCode": 200,
        "name": "CobaltEagle",
        "sequence": 1
      }
    ]
  },
  "available_vendors": [
    "CobaltEagle",
    "GoldenOtter",
    "AzureRaven",
    "SilverTiger",
    "EmeraldWhale",
    "CrimsonFalcon"
  ]
}
