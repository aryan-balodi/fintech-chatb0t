{
  "category": "ONBOARDING INDIVIDUAL",
  "service_name": "Driving License Verification",
  "description": "This service verifies driving license details, including personal and address information, for secure and compliant onboarding workflows.",
  "llm_summary": "Driving License Verification validates driving license details, providing personal, address, and vehicle class information for secure onboarding and compliance.",
  "tags": [
    "driving license verification",
    "identity verification",
    "onboarding api",
    "address verification",
    "vehicle classes",
    "secure kyc",
    "compliance"
  ],
  "use_cases": [
    "Verify Aadhaar-linked driving license details during individual onboarding.",
    "Retrieve personal and address information for KYC and compliance workflows.",
    "Validate vehicle class information for transportation and licensing purposes.",
    "Ensure accurate and secure identity verification for financial services.",
    "Support regulatory reporting with driving license data."
  ],
  "request_schema": [
    {
      "field": "drivingLicenceNumber",
      "type": "string",
      "description": "Driving license number to verify.",
      "validations": {
        "minLength": 1
      },
      "required": true
    },
    {
      "field": "dob",
      "type": "string",
      "description": "Date of birth of the license holder.",
      "validations": {
        "pattern": "\\d{4}-\\d{2}-\\d{2}"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing driving license data",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
      "field": "data",
      "type": "object",
      "description": "Driving license details",
      "properties": {
        "licenseNumber": {
          "type": "string",
          "description": "Driving license number"
        },
        "state": {
          "type": "string",
          "description": "State associated with the driving license"
        },
        "name": {
          "type": "string",
          "description": "Name of the license holder"
        },
        "permanentAddress": {
          "type": "string",
          "description": "Permanent address of the license holder"
        },
        "permanentZip": {
          "type": "string",
          "description": "Permanent address ZIP code"
        },
        "cityName": {
          "type": "string",
          "description": "City name of the license holder"
        },
        "temporaryAddress": {
          "type": "string",
          "description": "Temporary address of the license holder"
        },
        "temporaryZip": {
          "type": "string",
          "description": "Temporary address ZIP code"
        },
        "citizenship": {
          "type": "string",
          "description": "Citizenship of the license holder"
        },
        "olaName": {
          "type": "string",
          "description": "RTO name"
        },
        "olaCode": {
          "type": "string",
          "description": "RTO code"
        },
        "gender": {
          "type": "string",
          "description": "Gender of the license holder"
        },
        "fatherOrHusbandName": {
          "type": "string",
          "description": "Father's or husband's name of the license holder"
        },
        "dob": {
          "type": "string",
          "description": "Date of birth of the license holder"
        },
        "doe": {
          "type": "string",
          "description": "Date of expiry of the license"
        },
        "transportDoe": {
          "type": "string",
          "description": "Transport license expiry date"
        },
        "doi": {
          "type": "string",
          "description": "Date of issue of the license"
        },
        "transportDoi": {
          "type": "string",
          "description": "Transport license issue date"
        },
        "bloodGroup": {
          "type": "string",
          "description": "Blood group of the license holder"
        },
        "hasImage": {
          "type": "boolean",
          "description": "Indicates if the license has an image"
        },
        "vehicleClasses": {
          "type": "array",
          "description": "Vehicle classes associated with the license",
          "items": {
            "type": "string"
          }
        },
        "initialDoi": {
          "type": "string",
          "description": "Initial date of issue of the license"
        },
        "currentStatus": {
          "type": "string",
          "description": "Current status of the license"
        },
        "lessInfo": {
          "type": "boolean",
          "description": "Indicates if less information is available"
        }
      },
      "required": true
    },
    {
      "field": "vendorResponse",
      "type": "array",
      "description": "Response details from vendors",
      "items": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "integer",
            "description": "Vendor-specific status code"
          },
          "name": {
            "type": "string",
            "description": "Vendor name"
          },
          "sequence": {
            "type": "integer",
            "description": "Vendor sequence number"
          }
        }
      },
      "required": true
    }
  ],
  "example_request": {
