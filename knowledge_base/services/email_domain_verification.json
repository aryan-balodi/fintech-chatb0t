{
  "category": "ALTERNATE DATA SUITE",
  "service_name": "(Email) Domain Verification",
  "description": "This service verifies the details of a given email domain. It provides information such as domain age, registrar details, registrant information, technical contact, name servers, and status for alternate data-based verification workflows.",
  "llm_summary": "(Email) Domain Verification service validates an email domain and provides details like domain age, registrar, registrant, technical contact, and name servers. Useful for alternate data verification, fraud prevention, and compliance workflows.",
  "tags": [
    "email domain verification",
    "data verification",
    "compliance",
    "onboarding api",
    "secure kyc",
    "identity verification"
  ],
  "use_cases": [
    "Verify the validity and details of an email domain for onboarding workflows.",
    "Support compliance and fraud prevention by validating email domain information.",
    "Enable alternate data-based KYC processes in financial services and insurance.",
    "Validate email domains for identity verification workflows in government and private sectors."
  ],
  "request_schema": [
    {
      "field": "domainName",
      "type": "string",
      "description": "Domain name to verify.",
      "validations": {
        "format": "domain"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing domain verification data",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
      "field": "data",
      "type": "object",
      "description": "Details of the verified email domain",
      "properties": {
        "domainName": {
          "type": "string",
          "description": "The verified domain name"
        },
        "domainNameExt": {
          "type": "string",
          "description": "The domain extension"
        },
        "contactEmail": {
          "type": "string",
          "description": "Contact email for the domain"
        },
        "estimatedDomainAge": {
          "type": "string",
          "description": "Estimated age of the domain in days"
        },
        "createdDate": {
          "type": "string",
          "description": "Creation date of the domain"
        },
        "updatedDate": {
          "type": "string",
          "description": "Last updated date of the domain"
        },
        "expiresDate": {
          "type": "string",
          "description": "Expiration date of the domain"
        },
        "registrarName": {
          "type": "string",
          "description": "Name of the domain registrar"
        },
        "registrarIANAID": {
          "type": "string",
          "description": "IANA ID of the domain registrar"
        },
        "registrant": {
          "type": "object",
          "description": "Registrant details",
          "properties": {
            "organization": {
              "type": "string",
              "description": "Organization of the registrant"
            },
            "country": {
              "type": "string",
              "description": "Country of the registrant"
            },
            "countryCode": {
              "type": "string",
              "description": "Country code of the registrant"
            }
          }
        },
        "technicalContact": {
          "type": "object",
          "description": "Technical contact details",
          "properties": {
            "country": {
              "type": "string",
              "description": "Country of the technical contact"
            },
            "countryCode": {
              "type": "string",
              "description": "Country code of the technical contact"
            }
          }
        },
        "nameServers": {
          "type": "object",
          "description": "Name server details",
          "properties": {
            "hostNames": {
              "type": "array",
              "description": "List of name server hostnames",
              "items": {
                "type": "string"
              }
            },
            "ips": {
              "type": "array",
              "description": "List of name server IPs",
              "items": {
                "type": "string"
              }
            }
          }
        },
        "status": {
          "type": "string",
          "description": "Status of the domain"
        },
        "createdDateNormalized": {
          "type": "string",
          "description": "Normalized creation date of the domain"
