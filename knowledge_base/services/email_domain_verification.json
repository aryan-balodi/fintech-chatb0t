                  "items": {
                    "type": "string"
                  }
                }
              }
            },
            "status": {
              "type": "string",
              "description": "Status of the domain in the registry"
            },
            "createdDateNormalized": {
              "type": "string",
              "description": "Normalized creation date of the domain in the registry"
            },
            "updatedDateNormalized": {
              "type": "string",
              "description": "Normalized last updated date of the domain in the registry"
            },
            "expiresDateNormalized": {
              "type": "string",
              "description": "Normalized expiration date of the domain in the registry"
            },
            "registrarName": {
              "type": "string",
              "description": "Name of the registrar in the registry"
            },
            "registrarIANAID": {
              "type": "string",
              "description": "IANA ID of the registrar in the registry"
            },
            "whoisServer": {
              "type": "string",
              "description": "WHOIS server of the domain"
            }
          }
        }
      },
      "required": true
    },
    {
      "field": "vendorResponse",
      "type": "array",
