{
  "category": "ONBOARDING BUSINESS",
  "service_name": "MCA Company Details",
  "description": "This service retrieves detailed company information from the Ministry of Corporate Affairs (MCA) database using the Corporate Identification Number (CIN).",
  "llm_summary": "MCA Company Details provides comprehensive company information, including registration details, directors, charges, and compliance status, aiding in business onboarding and regulatory workflows.",
  "tags": [
    "mca company details",
    "business verification",
    "compliance",
    "onboarding api",
    "company information",
    "cin lookup"
  ],
  "use_cases": [
    "Retrieve company details for business onboarding.",
    "Validate company registration and compliance status.",
    "Support due diligence and risk assessment workflows.",
    "Ensure accurate business verification for regulatory purposes.",
    "Fetch director and charge details for financial analysis."
  ],
  "request_schema": [
    {
      "field": "cin",
      "type": "string",
      "description": "Corporate Identification Number (CIN) to fetch company details.",
      "validations": {
        "pattern": "^[A-Z]{1}[0-9]{5}[A-Z]{2}[0-9]{4}[A-Z]{3}[0-9]{6}$"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing MCA company details.",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response.",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API.",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed.",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service.",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
