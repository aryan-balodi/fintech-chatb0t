{
  "category": "ASSET VERIFICATION",
  "service_name": "Reverse RC Details (Chassis based)",
  "description": "This service retrieves detailed vehicle registration information based on the chassis number, including owner details, vehicle specifications, and insurance information. It supports asset verification and compliance workflows.",
  "llm_summary": "Reverse RC Details (Chassis based) retrieves vehicle registration details using the chassis number, aiding in asset verification and compliance.",
  "tags": [
    "reverse rc details",
    "chassis based verification",
    "asset verification",
    "vehicle details",
    "compliance",
    "insurance verification"
  ],
  "use_cases": [
    "Retrieve vehicle registration details using the chassis number for ownership validation.",
    "Ensure compliance with asset verification regulations.",
    "Retrieve vehicle specifications and insurance details for KYC processes.",
    "Support fraud prevention by validating chassis-based RC details."
  ],
  "request_schema": [
    {
      "field": "chassisNumber",
      "type": "string",
      "description": "Chassis number to retrieve RC details.",
      "validations": {
        "pattern": "^[A-Z0-9]{17}$"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for retrieving RC details.",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response.",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API.",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed.",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service.",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
