{
  "category": "ONBOARDING INDIVIDUAL",
  "service_name": "ITR Compliance Check (Pan based)",
  "description": "This service checks the Income Tax Return (ITR) compliance status of an individual using their PAN number, ensuring regulatory adherence and financial transparency.",
  "llm_summary": "ITR Compliance Check validates the compliance status of a PAN, including PAN-Aadhaar linkage, operability, and compliance with tax regulations.",
  "tags": [
    "ITR compliance",
    "PAN verification",
    "tax compliance",
    "financial transparency",
    "regulatory adherence",
    "onboarding api",
    "secure kyc"
  ],
  "use_cases": [
    "Verify ITR compliance status during individual onboarding.",
    "Ensure PAN-Aadhaar linkage for regulatory adherence.",
    "Validate PAN operability and compliance for financial services.",
    "Support tax-related workflows with accurate compliance data.",
    "Enhance KYC processes with ITR compliance checks."
  ],
  "request_schema": [
    {
      "field": "panNumber",
      "type": "string",
      "description": "Permanent Account Number (PAN) to verify.",
      "validations": {
        "pattern": "^[A-Z]{5}[0-9]{4}[A-Z]{1}$"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing ITR compliance data.",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response.",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API.",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed.",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service.",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
      "field": "data",
      "type": "object",
      "description": "ITR compliance details.",
      "properties": {
        "panNumber": {
          "type": "string",
          "description": "Permanent Account Number (PAN)."
        },
        "maskedName": {
          "type": "string",
          "description": "Masked name of the PAN holder."
        },
        "panAadhaarLinked": {
          "type": "string",
          "description": "Status of PAN-Aadhaar linkage."
        },
        "panStatus": {
          "type": "string",
          "description": "Operability status of the PAN."
        },
        "validPan": {
          "type": "string",
          "description": "Indicates if the PAN is valid."
        },
        "compliant": {
          "type": "string",
          "description": "Indicates if the PAN is compliant with tax regulations."
        },
        "specifiedPersonUnder206": {
          "type": "string",
          "description": "Indicates if the PAN holder is a specified person under Section 206."
        },
        "panAllotmentDate": {
          "type": "string",
          "description": "Date of PAN allotment."
        }
      },
      "required": true
    },
    {
      "field": "vendorResponse",
      "type": "array",
      "description": "Response details from vendors.",
      "items": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "integer",
            "description": "Vendor-specific status code."
          },
          "name": {
            "type": "string",
            "description": "Vendor name."
          },
          "sequence": {
            "type": "integer",
            "description": "Vendor sequence number."
          }
        }
      },
      "required": true
    }
  ],
  "example_request": {
    "input": {
      "panNumber": "CDZPP6767P",
      "consent": true
    }
  },
  "example_response": {
    "status": "success",
    "requestId": "525e54ea-8c88-4821-bd76-7848daaf23b5",
    "type": "point",
    "serviceStatusCode": 200,
    "data": {
      "panNumber": "CDZPP6767P",
      "maskedName": "PXXXXL VXXXD PXXXN",
      "panAadhaarLinked": "linked",
      "panStatus": "operative",
      "validPan": "true",
      "compliant": "true",
      "specifiedPersonUnder206": "no",
      "panAllotmentDate": ""
    },
    "vendorResponse": [
      {
        "statusCode": 200,
        "name": "AzureRaven",
        "sequence": 1
      }
    ]
  },
  "available_vendors": [
    "OnyxWolf",
    "SapphireSwan",
    "CobaltEagle",
    "SilverTiger",
    "AzureRaven",
    "ScarletPanther",
    "EmeraldWhale",
    "GoldenOtter",
    "CrimsonFalcon"
  ]
}
