{
  "category": "ALTERNATE DATA SUITE",
  "service_name": "Phone to (Alternate) Phone",
  "description": "This service retrieves alternate phone numbers associated with a given phone number. It provides a list of alternate phone numbers, seen dates, and other relevant details for alternate data-based verification workflows.",
  "llm_summary": "Phone to (Alternate) Phone service maps a phone number to a list of alternate phone numbers, including seen dates. Useful for alternate data verification, fraud prevention, and compliance workflows.",
  "tags": [
    "phone to phone",
    "alternate phone",
    "data verification",
    "compliance",
    "onboarding api",
    "secure kyc"
  ],
  "use_cases": [
    "Retrieve alternate phone numbers linked to a given phone number for onboarding workflows.",
    "Support compliance and fraud prevention by verifying alternate phone information.",
    "Enable alternate data-based KYC processes in financial services and insurance.",
    "Verify alternate phone numbers for identity verification workflows in government and private sectors."
  ],
  "request_schema": [
    {
      "field": "mobileNumber",
      "type": "string",
      "description": "Mobile number to map to alternate phone numbers.",
      "validations": {
        "minLength": 10,
        "maxLength": 10,
        "pattern": "\\d{10}"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing alternate phone data",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
      "field": "data",
      "type": "object",
      "description": "Alternate phone data associated with the phone number",
      "properties": {
        "phoneNumberCount": {
          "type": "integer",
          "description": "Number of alternate phone numbers associated with the phone number"
        },
        "phones": {
          "type": "array",
          "description": "List of alternate phone numbers associated with the phone number",
          "items": {
            "type": "object",
            "properties": {
              "seenDate": {
                "type": "string",
                "description": "Date when the phone number was seen"
              },
              "phoneNumber": {
                "type": "string",
                "description": "Alternate phone number"
              }
            }
          }
        }
      },
      "required": true
    },
    {
      "field": "vendorResponse",
      "type": "array",
      "description": "Response details from vendors",
      "items": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "integer",
            "description": "Vendor-specific status code"
          },
          "name": {
            "type": "string",
            "description": "Vendor name"
          },
          "sequence": {
            "type": "integer",
            "description": "Vendor sequence number"
          }
        }
      },
      "required": true
    }
  ],
  "example_request": {
    "input": {
      "mobileNumber": "9870963498",
      "consent": true
    }
  },
  "example_response": {
    "status": "success",
    "requestId": "10ffa9a7-52a4-442d-9b7f-36e7637c0518",
    "type": "point",
    "serviceStatusCode": 200,
    "data": {
      "phoneNumberCount": 3,
      "phones": [
        {
          "seenDate": "30-06-2025",
          "phoneNumber": "00919870963498"
        },
        {
          "seenDate": "30-06-2025",
          "phoneNumber": "9870963498"
        },
        {
          "seenDate": "30-06-2025",
          "phoneNumber": "49204920"
        }
      ]
    },
    "vendorResponse": [
      {
        "statusCode": 200,
        "name": "AzureRaven",
        "sequence": 1
      }
    ]
  },
  "available_vendors": [
    "AzureRaven",
    "JadeToucan",
    "CobaltEagle",
    "GoldenOtter",
    "SilverTiger",
    "EmeraldWhale",
    "CrimsonFalcon"
  ]
}
