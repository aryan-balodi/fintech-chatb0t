{
  "category": "EMPLOYMENT VERIFICATION",
  "service_name": "mobile to UAN",
  "description": "This service help you find the UAN number using the linked mobile number (EPFO based).",
  "llm_summary": "mobile to UAN maps a given mobile number to corresponding UAN(s) issued by EPFO, assisting employment verification and compliance. Requires unique client request ID.",
  "tags": [
    "mobile to uan",
    "employment verification",
    "epfo",
    "uan lookup",
    "compliance",
    "identity mapping"
  ],
  "use_cases": [
    "Retrieve UAN(s) associated with a given mobile number for employee verification.",
    "Cross-check employment records during onboarding processes.",
    "Facilitate compliance checks based on EPFO-linked UAN data.",
    "Assist payroll and HR systems in validating employee identities."
  ],
  "request_schema": [
    {
      "field": "mobileNumber",
      "type": "string",
      "description": "Mobile number to fetch UAN",
      "validations": {
        "pattern": "^\\d{10}$"
      },
      "required": true
    },
    {
      "field": "client_request_id",
      "type": "string",
      "description": "Unique identifier for the client request",
      "validations": {
        "minLength": 1
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "request_id",
      "type": "string",
      "description": "Unique request ID generated by API",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "client_request_id",
      "type": "string",
      "description": "Client-provided request ID",
      "validations": {
        "minLength": 1
      },
      "required": true
    },
    {
      "field": "result",
      "type": "array",
      "description": "List of UANs associated with the mobile number",
      "items": {
        "type": "string",
        "validations": {
          "minLength": 1
        }
      },
      "required": true
    },
    {
      "field": "statusCode",
      "type": "string",
      "description": "Status code for the result",
      "validations": {
        "pattern": "^[0-9]+$"
      },
      "required": true
    }
  ],
  "example_request": {
    "input": {
    "mobileNumber": "8888888888"
  },
  "client_request_id": "sample_request_123"
  },
  "example_response": {
    "request_id": "12345abcde-6789",
    "client_request_id": "sample_request_123",
    "data": {
        "status": "success",
        "result": {
            "result": [
                "101569109538"
            ],
            "statusCode": "200"
        }
    }
  },
  "available_vendors": [
    "OnyxWolf",
    "SapphireSwan",
    "CobaltEagle",
    "SilverTiger",
    "AzureRaven",
    "ScarletPanther",
    "EmeraldWhale",
    "GoldenOtter",
    "CrimsonFalcon"
  ]
}
