{
  "category": "ONBOARDING BUSINESS",
  "service_name": "GST Turnover",
  "description": "This service retrieves GST turnover details, including filed and estimated totals, for compliance and financial analysis workflows.",
  "llm_summary": "GST Turnover provides detailed turnover information based on GSTIN and year, including filed and estimated totals, aiding in business onboarding and compliance.",
  "tags": [
    "gst turnover",
    "business verification",
    "compliance",
    "financial analysis",
    "onboarding api",
    "gstin lookup"
  ],
  "use_cases": [
    "Retrieve GST turnover details for financial analysis.",
    "Validate turnover information during business onboarding.",
    "Support compliance workflows with accurate turnover data.",
    "Assist in due diligence and risk assessment for financial services.",
    "Ensure accurate business verification for regulatory purposes."
  ],
  "request_schema": [
    {
      "field": "gstin",
      "type": "string",
      "description": "GSTIN (Goods and Services Tax Identification Number) to fetch turnover details.",
      "validations": {
        "pattern": "^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$"
      },
      "required": true
    },
    {
      "field": "year",
      "type": "string",
      "description": "Financial year for which turnover details are required.",
      "validations": {
        "pattern": "^\\d{4}-\\d{4}$"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing GST turnover details.",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response.",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API.",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed.",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service.",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
      "field": "data",
      "type": "object",
      "description": "GST turnover details.",
      "properties": {
        "gstFiledTotal": {
          "type": "string",
          "description": "Total GST filed turnover."
        },
        "panFiledTotal": {
          "type": "string",
          "description": "Total PAN filed turnover."
        },
        "gstEstimatedTotal": {
          "type": "string",
          "description": "Total GST estimated turnover."
        },
        "panEstimatedTotal": {
          "type": "string",
          "description": "Total PAN estimated turnover."
        }
      },
      "required": true
    },
    {
      "field": "vendorResponse",
      "type": "array",
      "description": "Response details from vendors.",
      "items": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "integer",
            "description": "Vendor-specific status code."
          },
          "name": {
            "type": "string",
            "description": "Vendor name."
          },
          "sequence": {
            "type": "integer",
            "description": "Vendor sequence number."
          }
        }
      },
      "required": true
    }
  ],
  "example_request": {
    "input": {
      "gstin": "27AAACR5055K1Z7",
      "year": "2024-2025",
      "consent": true
    }
  },
  "example_response": {
    "status": "success",
    "requestId": "a22719ad-b132-4ba0-a260-cec85e43e53f",
    "type": "point",
    "serviceStatusCode": 200,
    "data": {
      "gstFiledTotal": "",
      "panFiledTotal": "6640677918090.72",
      "gstEstimatedTotal": "",
      "panEstimatedTotal": "6645262221131.91"
    },
    "vendorResponse": [
      {
        "statusCode": 200,
        "name": "AzureRaven",
        "sequence": 1
      }
    ]
  },
  "available_vendors": [
    "OnyxWolf",
