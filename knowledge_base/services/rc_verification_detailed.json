{
  "category": "ASSET VERIFICATION",
  "service_name": "RC Verification (Detailed)",
  "description": "This service provides detailed verification of a vehicle's Registration Certificate (RC), including owner details, vehicle specifications, insurance, and tax information. It ensures compliance and supports asset verification workflows.",
  "llm_summary": "RC Verification (Detailed) validates a vehicle's RC details, including owner information, vehicle specifications, and insurance, aiding in asset verification and compliance.",
  "tags": [
    "rc verification",
    "vehicle verification",
    "asset verification",
    "compliance",
    "vehicle details",
    "insurance verification"
  ],
  "use_cases": [
    "Verify detailed RC information for vehicle ownership validation.",
    "Ensure compliance with asset verification regulations.",
    "Retrieve vehicle specifications and insurance details for KYC processes.",
    "Support fraud prevention by validating RC details."
  ],
  "request_schema": [
    {
      "field": "vehicleNumber",
      "type": "string",
      "description": "Vehicle number to verify the RC details.",
      "validations": {
        "pattern": "^[A-Z]{2}[0-9]{2}[A-Z]{2}[0-9]{4}$"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for verifying the RC details.",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response.",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API.",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed.",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service.",
      "validations": {
        "minimum": 100,
