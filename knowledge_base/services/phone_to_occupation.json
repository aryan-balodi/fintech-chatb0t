{
  "category": "ALTERNATE DATA SUITE",
  "service_name": "Phone to Occupation",
  "description": "This service identifies the occupation of an individual based on their phone number. It enables alternate data-based verification workflows by linking phone numbers to occupational details.",
  "llm_summary": "Phone to Occupation service maps a phone number to the associated occupation of the individual. Useful for alternate data verification and compliance workflows.",
  "tags": [
    "phone to occupation",
    "alternate data",
    "occupation verification",
    "compliance",
    "onboarding api",
    "data verification",
    "secure kyc"
  ],
  "use_cases": [
    "Verify the occupation of an individual during onboarding workflows.",
    "Retrieve occupational details for compliance and fraud prevention.",
    "Support alternate data-based KYC processes in financial services and insurance.",
    "Enable identity verification workflows in government and private sectors."
  ],
  "request_schema": [
    {
      "field": "mobileNumber",
      "type": "string",
      "description": "Mobile number to map to occupation.",
      "validations": {
        "minLength": 10,
        "maxLength": 10,
        "pattern": "\\d{10}"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing occupation data",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed",
      "validations": {
        "allowedValues": ["point"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
      "field": "data",
      "type": "object",
      "description": "Occupation data",
      "properties": {
        "occupation": {
          "type": "string",
          "description": "Occupation of the individual"
        }
      },
      "required": true
    },
    {
      "field": "vendorResponse",
      "type": "array",
      "description": "Response details from vendors",
      "items": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "integer",
            "description": "Vendor-specific status code"
          },
          "name": {
            "type": "string",
            "description": "Vendor name"
          },
          "sequence": {
            "type": "integer",
            "description": "Vendor sequence number"
          }
        }
      },
      "required": true
    }
  ],
  "example_request": {
    "input": {
      "mobileNumber": "9050271219",
      "consent": true
    }
  },
  "example_response": {
    "status": "success",
    "requestId": "41c69280-0c9f-4724-9dce-8bd7799e119b",
    "type": "point",
    "serviceStatusCode": 200,
    "data": {
      "occupation": "selfEmployed"
    },
    "vendorResponse": [
      {
        "statusCode": 200,
        "name": "CobaltEagle",
        "sequence": 1
      }
    ]
  },
  "available_vendors": [
    "CobaltEagle",
    "GoldenOtter",
    "AzureRaven",
    "SilverTiger",
    "EmeraldWhale",
    "CrimsonFalcon"
  ]
}
