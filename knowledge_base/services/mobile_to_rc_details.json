{
  "category": "ASSET VERIFICATION",
  "service_name": "Mobile to RC Details",
  "description": "This service retrieves detailed RC (Registration Certificate) information associated with a given mobile number. It provides comprehensive vehicle details for asset verification workflows, ensuring accurate and secure data retrieval.",
  "llm_summary": "Mobile to RC Details service maps a mobile number to detailed vehicle registration information, including owner details, vehicle specifications, and registration status. Ideal for asset verification and compliance workflows.",
  "tags": [
    "mobile to rc details",
    "asset verification",
    "vehicle registration",
    "compliance",
    "onboarding api",
    "rc details",
    "secure kyc"
  ],
  "use_cases": [
    "Retrieve detailed vehicle information for asset verification workflows.",
    "Support compliance and fraud prevention by linking mobile numbers to vehicle details.",
    "Enable KYC processes in financial services, insurance, and government sectors.",
    "Verify vehicle ownership and registration status for regulatory reporting."
  ],
  "request_schema": [
    {
      "field": "mobileNumber",
      "type": "string",
      "description": "Mobile number to map to RC details.",
      "validations": {
        "minLength": 10,
        "maxLength": 10,
        "pattern": "\\d{10}"
      },
      "required": true
    },
    {
      "field": "consent",
      "type": "boolean",
      "description": "User consent for accessing RC-linked data",
      "validations": {
        "allowedValues": [true, false]
      },
      "required": true
    }
  ],
  "response_schema": [
    {
      "field": "status",
      "type": "string",
      "description": "Overall status of the API response",
      "validations": {
        "allowedValues": ["success", "error", "failed"]
      },
      "required": true
    },
    {
      "field": "requestId",
      "type": "string",
      "description": "Unique request ID generated by the API",
      "validations": {
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
      },
      "required": true
    },
    {
      "field": "type",
      "type": "string",
      "description": "Type of verification performed",
      "validations": {
        "allowedValues": ["composite"]
      },
      "required": true
    },
    {
      "field": "serviceStatusCode",
      "type": "integer",
      "description": "HTTP status code for the service",
      "validations": {
        "minimum": 100,
        "maximum": 599
      },
      "required": true
    },
    {
      "field": "data",
      "type": "object",
      "description": "Detailed RC information",
      "properties": {
        "vehicles": {
          "type": "array",
          "description": "List of vehicles associated with the mobile number",
          "items": {
            "type": "object",
            "properties": {
              "vehicleNumber": { "type": "string", "description": "Vehicle registration number" },
              "chassisNumber": { "type": "string", "description": "Chassis number of the vehicle" },
              "engineNumber": { "type": "string", "description": "Engine number of the vehicle" },
              "manufacturer": { "type": "string", "description": "Vehicle manufacturer" },
              "model": { "type": "string", "description": "Vehicle model" },
              "vehicleColor": { "type": "string", "description": "Color of the vehicle" },
              "fuelType": { "type": "string", "description": "Fuel type of the vehicle" },
              "emissionNorms": { "type": "string", "description": "Emission norms compliance" },
              "bodyType": { "type": "string", "description": "Body type of the vehicle" },
              "ownerCount": { "type": "string", "description": "Number of previous owners" },
              "ownerName": { "type": "string", "description": "Name of the vehicle owner" },
              "ownerFatherName": { "type": "string", "description": "Father's name of the vehicle owner" },
              "mobileNumber": { "type": "string", "description": "Mobile number linked to the vehicle" },
              "rcStatus": { "type": "string", "description": "Status of the RC" },
              "rcStatusAsOn": { "type": "string", "description": "Date of the RC status" },
              "registrationAuthority": { "type": "string", "description": "Registration authority details" },
              "registrationDate": { "type": "string", "description": "Date of vehicle registration" },
              "manufacturingMonthYear": { "type": "string", "description": "Manufacturing month and year" },
              "rcExpiryDate": { "type": "string", "description": "Expiry date of the RC" },
              "taxUpto": { "type": "string", "description": "Tax validity date" },
              "insuranceCompany": { "type": "string", "description": "Insurance company name" },
              "insuranceUpto": { "type": "string", "description": "Insurance validity date" },
              "insurancePolicyNumber": { "type": "string", "description": "Insurance policy number" },
              "rcFinancer": { "type": "string", "description": "RC financer details" },
              "presentAddress": { "type": "string", "description": "Present address of the owner" },
              "permanentAddress": { "type": "string", "description": "Permanent address of the owner" },
              "cubicCapacity": { "type": "string", "description": "Cubic capacity of the vehicle" },
              "grossWeight": { "type": "string", "description": "Gross weight of the vehicle" },
              "unladenWeight": { "type": "string", "description": "Unladen weight of the vehicle" },
              "vehicleCategory": { "type": "string", "description": "Category of the vehicle" },
              "standardCap": { "type": "string", "description": "Standard capacity of the vehicle" },
              "cylinders": { "type": "string", "description": "Number of cylinders in the vehicle" },
              "seatCapacity": { "type": "string", "description": "Seating capacity of the vehicle" },
              "sleeperCapacity": { "type": "string", "description": "Sleeper capacity of the vehicle" },
              "standingCapacity": { "type": "string", "description": "Standing capacity of the vehicle" },
              "wheelbase": { "type": "string", "description": "Wheelbase of the vehicle" },
              "puccNumber": { "type": "string", "description": "PUCC number of the vehicle" },
              "puccUpto": { "type": "string", "description": "PUCC validity date" },
              "blacklistStatus": { "type": "boolean", "description": "Blacklist status of the vehicle" },
              "permitIssueDate": { "type": "string", "description": "Permit issue date" },
              "permitNumber": { "type": "string", "description": "Permit number" },
              "permitType": { "type": "string", "description": "Type of permit" },
              "permitValidFrom": { "type": "string", "description": "Permit validity start date" },
              "permitValidUpto": { "type": "string", "description": "Permit validity end date" },
              "nonUseStatus": { "type": "string", "description": "Non-use status of the vehicle" },
              "nonUseFrom": { "type": "string", "description": "Non-use start date" },
              "nonUseTo": { "type": "string", "description": "Non-use end date" },
              "nationalPermitNumber": { "type": "string", "description": "National permit number" },
              "nationalPermitUpto": { "type": "string", "description": "National permit validity date" },
              "nationalPermitIssuedBy": { "type": "string", "description": "National permit issuing authority" },
              "isCommercial": { "type": "boolean", "description": "Indicates if the vehicle is commercial" },
              "nocDetails": { "type": "string", "description": "NOC details of the vehicle" },
              "dbResult": { "type": "boolean", "description": "Database result status" },
              "partialData": { "type": "boolean", "description": "Indicates if partial data is available" },
              "class": { "type": "string", "description": "Class of the vehicle" }
            }
          }
        }
      },
      "required": true
    },
    {
      "field": "vendorResponse",
      "type": "array",
      "description": "Response details from vendors",
      "items": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "integer",
            "description": "Vendor-specific status code"
          },
          "name": {
            "type": "string",
            "description": "Vendor name"
          },
          "sequence": {
            "type": "integer",
            "description": "Vendor sequence number"
          }
        }
      },
      "required": true
    }
  ],
